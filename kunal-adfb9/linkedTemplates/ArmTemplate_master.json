				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_key_vault_sql_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('ls_key_vault_sql_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'kunal-adfb9_ArmTemplate_1')]"
			]
		},
		{
			"name": "kunal-adfb9_ArmTemplate_3",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_3.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"tumbling_trigger_properties_pipeline_parameters_StartDate": {
						"value": "[parameters('tumbling_trigger_properties_pipeline_parameters_StartDate')]"
					},
					"tumbling_trigger_properties_pipeline_parameters_EndDate": {
						"value": "[parameters('tumbling_trigger_properties_pipeline_parameters_EndDate')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'kunal-adfb9_ArmTemplate_2')]"
			]
		}
