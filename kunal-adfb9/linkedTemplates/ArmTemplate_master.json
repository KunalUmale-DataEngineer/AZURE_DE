					},
					"ls_tgt_sql_srv_properties_typeProperties_database": {
						"value": "[parameters('ls_tgt_sql_srv_properties_typeProperties_database')]"
					},
					"ls_tgt_sql_srv_properties_typeProperties_userName": {
						"value": "[parameters('ls_tgt_sql_srv_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "kunal-adfb9_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
