{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "kunal-adfb9"
		},
		"ls_adls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls'"
		},
		"ls_sql_server_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_sql_server'"
		},
		"ls_tgt_sql_srv_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_tgt_sql_srv'"
		},
		"ls_adls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://kunalstgb9.dfs.core.windows.net/"
		},
		"ls_key_vault_sql_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "kunalsql"
		},
		"ls_sql_key_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://Kvault9833.vault.azure.net/"
		},
		"ls_tgt_sql_srv_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "kunalb9.database.windows.net"
		},
		"ls_tgt_sql_srv_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "tgt_azure"
		},
		"ls_tgt_sql_srv_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "kunal"
		},
		"tumbling_trigger_properties_pipeline_parameters_StartDate": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowStartTime"
		},
		"tumbling_trigger_properties_pipeline_parameters_EndDate": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowEndTime"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "kunal-adfb9_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adls_accountKey": {
						"value": "[parameters('ls_adls_accountKey')]"
					},
					"ls_sql_server_connectionString": {
						"value": "[parameters('ls_sql_server_connectionString')]"
					},
					"ls_tgt_sql_srv_password": {
						"value": "[parameters('ls_tgt_sql_srv_password')]"
					},
					"ls_adls_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_properties_typeProperties_url')]"
					},
					"ls_sql_key_properties_typeProperties_baseUrl": {
