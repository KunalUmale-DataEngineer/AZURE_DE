					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'kunal-adfb9_ArmTemplate_0')]"
			]
		},
		{
			"name": "kunal-adfb9_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
